{% extends 'base.html.twig' %}

{% block title %}Log in{% endblock %}

{% block body %}
    <h1>Log in</h1>

    {{ form_start(loginForm) }}
        {{ form_row(loginForm.email) }}
        {{ form_row(loginForm.password) }}
        <button type="submit">Log in</button>
    {{ form_end(loginForm) }}
{% endblock %}